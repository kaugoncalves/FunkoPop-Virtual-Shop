}

.efeito{

border: none;
padding: 20px;
margin: 10px;
font-size: 20px;
border-radius: 10px;
position: relative;
box-sizing: border-box;
cursor: pointer;
transition: all 200ms ease;

}
.animate-carrinho{
  color: white;
  background: none;
  transform-style: preserve-3d;
  
}
.animate-carrinho::after{
  top: -100%;
  left: 0px;
  width:100%;
  padding: 20px;
  position: absolute;
  background: greenyellow;
  color: black;
  font-size: 20px;
  border-radius: 10px;
  box-sizing: border-box;
  content:'ðŸ›’Carrinho';
  transform-origin: left bottom;
  transform: rotateX(90deg);
}
.animate-carrinho:hover{
  transform-origin: center bottom;
  transform: rotateX(-90deg) translateY(100%);
}

.animate-sair{
  color: white;
  background: none;
  transform-style: preserve-3d;
  
}
.animate-sair::after{
  top: -100%;
  left: 0px;
  width:100%;
  padding: 20px;
  position: absolute;
  background: red;
  color: black;
 
  border-radius: 10px;
  box-sizing: border-box;
  content:'ðŸ“´Deslogar';
  transform-origin: left bottom;
  transform: rotateX(90deg);
}
.animate-sair:hover{
  transform-origin: center bottom;
  transform: rotateX(-90deg) translateY(100%);
}


.animate-adicionar{
background: none;
color: rgb(37, 37, 37);
width: 150px;
height: 25px;
border: none;
color: black;
border-radius: 4px;
transition: ease-out 0.3s;
font-size: 12px;
outline: none;
border: 3px solid  rgb(72, 72, 202);;
position: relative;
z-index: 1;
}
.animate-adicionar:hover{
  color: #fff;
  cursor: pointer;
}
.animate-adicionar::before{
  transition: 0.5s all ease;
  position: absolute;
  top: 0;
  left: 50%;
  right: 50%;
  bottom: 0;
  opacity: 0;
  content: "";
  background-color: rgb(72, 72, 202);;
}
.animate-adicionar:hover:before{
  transition: 0.5s all ease;
  left: 0;
  right: 0;
  opacity: 1;
  z-index: -1;
}

.border-card {
  border: solid 5px transparent;
  transition: border-width 0.6s linear;
  
}

.border-card:hover { border-width: 10px;
}

.animate-card { 
  border: 5px solid blueviolet;
  animation: mymove 1s infinite;
}

@keyframes mymove {
  50% {border-color: blueviolet;}
}

.animate-font {  
  width: 300px;
  color: black;
  animation: mymove2 2s infinite;
}

@keyframes mymove2 {
  50% {color: blue;}
}


  </style>

  

